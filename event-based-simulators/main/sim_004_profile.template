{
    "id": "",
    "alarms": [],
    "type": "MACHINE",
    "deviceId": "${deviceId}",
    "shortShutdowns": 120,
    "version": "2.0.0",
    "amountUnit": "",
    "name": "Simulator Normal with Short Shutdowns - ${counter}",
    "tenantId": "${tenantId}",
    "locationId": "Matrix",
    "external": false,
    "testConfiguration": false,
    "subscriptions": [
        {
            "pollingIntervalSeconds": 10,
            "name": "Piece_Ok",
            "type": "event",
            "deviceID": "${deviceId}"
        },
        {
            "pollingIntervalSeconds": 10,
            "name": "Piece_Produced",
            "type": "event",
            "deviceID": "${deviceId}"
        },
        {
            "pollingIntervalSeconds": 10,
            "name": "Availability",
            "type": "event",
            "deviceID": "${deviceId}"
        }
    ],
    "inputs": {
        "ActualQualityAmount": [
            {
                "type": "TRANSFORMATION_RULE",
                "value": "countEvents(\"${deviceId}\",\"Piece_Ok\")"
            }
        ],
        "ActualProductionAmount": [
            {
                "type": "TRANSFORMATION_RULE",
                "value": "countEvents(\"${deviceId}\",\"Piece_Produced\")"
            }
        ],
        "ActualProductionTime": [
            {
                "type": "MACHINE_EVENT",
                "value": ""
            }
        ]
    },
    "intervals": [
        1800.0,
        3600.0
    ],
    "timeframes": {
        "Interval1": "30",
        "IntervalUnit1": "min",
        "Interval2": "1",
        "IntervalUnit2": "h",
        "Interval3": null,
        "IntervalUnit3": null,
        "Interval4": null,
        "IntervalUnit4": null     
    },
    "oeeTargets": {
        "overall": 60,
        "availability": 90,
        "performance": 90,    
        "quality": 80
    },
    "ui": {
        "shortStoppages": "",
        "correlationSectionVisited": true,
        "computation": "PPQ",
        "shortStoppagesAmount": "3"
    },
    "idealCycleAmount": "",
    "machineEvents": {
        "MACHINE_STATUS": [
            {
                "value": "evt(\"${deviceId}\",\"Availability\",\"status\",false) = \"up\""
            }
        ],
        "QUALITY_STATUS": []
    },
    "workpiece": {
        "amount": "25",
        "unit": "pcs",
        "name": "Pieces",
        "isActive": true,
        "timeunit": 2
    },
    "status": "INCOMPLETE"
}